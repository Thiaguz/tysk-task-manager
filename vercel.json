{
    "version": 2,
    "builds": [{
        "use": "@now/node",
        "src": "build/server.js"
    }],
    "public": false,
    "routes": [
        {
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "*",
                "Access-Control-Allow-Headers": "*"
            },
            "src": "/.*",
            "dest": "build/server.js"
        }
    ]
}
